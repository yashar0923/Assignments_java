public class AlphabetChecker {
    public static boolean containsAllAlphabetLetters(String input) {
        // Convert the input string to lowercase for case-insensitive comparison
        String lowercaseInput = input.toLowerCase();

        // Create a boolean array to track the presence of each letter
        boolean[] lettersPresent = new boolean[26];

        // Iterate over each character in the input string
        for (int i = 0; i < lowercaseInput.length(); i++) {
            char ch = lowercaseInput.charAt(i);

            // Check if the character is a letter between 'a' and 'z'
            if (Character.isLetter(ch)) {
                // Set the corresponding index in the boolean array to true
                lettersPresent[ch - 'a'] = true;
            }
        }

        // Check if all letters from 'a' to 'z' are present
        for (boolean letter : lettersPresent) {
            if (!letter) {
                return false;
            }
        }

        return true;
    }

    public static void main(String[] args) {
        String input = "The quick brown fox jumps over the lazy dog";
        boolean containsAllLetters = containsAllAlphabetLetters(input);
        System.out.println("Contains all letters: " + containsAllLetters);
    }
}

