import java.time.LocalDate;
import java.time.format.DateTimeFormatter;
import java.util.Scanner;

public class KYCFormDateRange {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = Integer.parseInt(scanner.nextLine());

        DateTimeFormatter formatter = DateTimeFormatter.ofPattern("dd-MM-yyyy");
        for (int i = 0; i < n; i++) {
            String[] dates = scanner.nextLine().split(" ");
            LocalDate signupDate = LocalDate.parse(dates[0], formatter);
            LocalDate currentDate = LocalDate.parse(dates[1], formatter);

            LocalDate anniversaryDate = signupDate.withYear(currentDate.getYear());
            LocalDate anniversaryMinus30Days = anniversaryDate.minusDays(30);
            LocalDate anniversaryPlus30Days = anniversaryDate.plusDays(30);

            if (currentDate.isBefore(anniversaryMinus30Days)) {
                System.out.println(formatter.format(anniversaryMinus30Days) + " " + formatter.format(anniversaryDate.minusDays(1)));
            } else if (currentDate.isAfter(anniversaryPlus30Days)) {
                System.out.println(formatter.format(anniversaryDate.plusDays(1)) + " " + formatter.format(anniversaryPlus30Days));
            } else {
                System.out.println(formatter.format(anniversaryMinus30Days) + " " + formatter.format(anniversaryPlus30Days));
            }
        }
        scanner.close();
    }
}

